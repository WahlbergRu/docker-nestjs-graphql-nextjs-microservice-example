{
  "name": "api-gateway",
  "version": "1.0.0",
  "description": "GraphQL API Gateway",
  "main": "api-gateway",
  "scripts": {
    "prebuild": "rimraf -G ./dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint --max-warnings=0 \"{src,__tests__}/**/*.ts\"",
    "lint:fix": "eslint --fix \"{src,__tests__}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "generate:typings": "ts-node ./src/graphql/generate-typings",
    "copy:protos": "cpy \"../_proto\" \"./src/_proto\""
  },
  "dependencies": {
    "@apollo/gateway": "^2.2.1",
    "@grpc/grpc-js": "^1.7.3",
    "@grpc/proto-loader": "^0.7.3",
    "@nestjs/apollo": "^10.1.6",
    "@nestjs/common": "9.2.1",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.2.1",
    "@nestjs/graphql": "^10.1.6",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/microservices": "9.2.1",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.2.1",
    "apollo-server-express": "^3.11.1",
    "bcryptjs": "2.4.3",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "graphql": "^16.6.0",
    "graphql-redis-subscriptions": "2.5.0",
    "graphql-scalars": "1.20.1",
    "graphql-subscriptions": "2.0.0",
    "graphql-tools": "8.3.12",
    "graphql-type-json": "0.3.2",
    "ioredis": "5.2.4",
    "lodash": "4.17.21",
    "nestjs-pino": "3.1.1",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "pino-http": "8.2.1",
    "pino-pretty": "^9.1.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.7",
    "ts-morph": "^16.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "9.1.5",
    "@nestjs/schematics": "9.0.3",
    "@nestjs/testing": "9.2.1",
    "@types/bcryptjs": "2.4.2",
    "@types/faker": "6.6.9",
    "@types/ioredis": "5.0.0",
    "@types/jest": "29.2.3",
    "@types/lodash": "4.14.190",
    "@types/node": "18.11.9",
    "@types/passport-jwt": "3.0.7",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "cpy-cli": "4.2.0",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.6",
    "eslint-plugin-prettier": "4.2.1",
    "faker": "6.6.6",
    "jest": "29.3.1",
    "jest-extended": "3.2.0",
    "prettier": "2.8.0",
    "rimraf": "3.0.2",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:WahlbergRu/docker-nestjs-graphql-nextjs-microservice-example"
