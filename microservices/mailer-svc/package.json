    "nodemailer": "6.8.0",
    "nestjs-pino": "3.1.1",
    "pino-http": "8.2.1",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@nestjs/cli": "9.1.5",
    "@nestjs/schematics": "9.0.3",
