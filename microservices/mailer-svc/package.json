{
  "name": "mailer-svc",
  "main": "microservices/mailer-svc",
  "version": "1.0.0",
  "description": "gRPC microservice back-end for mailer. Used for learning/trial purposes only.",
  "scripts": {
    "prebuild": "rimraf -G ./dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint --max-warnings=0 '{src,__tests__}/**/*.ts'",
    "lint:fix": "eslint --fix '{src,__tests__}/**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "copy:protos": "cpy ../../_proto ./src/_proto"
  },
  "dependencies": {
